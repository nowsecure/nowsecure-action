{
  "name": "nowsecure-action",
  "version": "2.1.1",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "Keegan Saunders <ksaunders@nowsecure.com>",
  "scripts": {
    "build": "genversion -d --es6 --semi src/nowsecure-version.ts && tsc",
    "lint": "eslint src/**/*.ts && prettier --check **/*.ts",
    "format": "prettier --write .",
    "prepare-release": "yarn build && ncc build -o dist/nowsecure-upload-app -s src/nowsecure-upload-app.ts && ncc build -o dist/nowsecure-convert-sarif -s src/nowsecure-convert-sarif.ts",
    "test": "jest"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.7",
    "@types/sarif": "2.1.4",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "@vercel/ncc": "0.33.1",
    "eslint": "8.6.0",
    "genversion": "3.0.2",
    "jest": "27.5.1",
    "nock": "13.2.4",
    "prettier": "2.5.1",
    "ts-jest": "27.1.4",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "@actions/core": "1.9.1",
    "@actions/github": "5.0.1",
    "@actions/http-client": "1.0.11",
    "ripgrep-js": "3.0.0",
    "yaml": "1.10.2"
  }
}
